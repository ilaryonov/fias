package main

import "encoding/xml"

type AddrObject struct {
 Id string
 Address string
}

type HouseObject struct {
 HouseId string
 HouseNumber string
}

type ParseElement func(decoder *xml.Decoder) interface{}

func ParseFile(parseElement ParseElement) interface{} {
 decoder.Prepare()

 return parseElement(decoder)
}

func ParseAddress() {
 address := ParseFile(func(decoder *xml.Decoder) interface{} {
  result := AddrObject{}
  decoder.DecodeElement(result, element)

  return result
 }).(AddrObject)
}